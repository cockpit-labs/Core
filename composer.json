{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1",
        "doctrine/annotations": "^1",
        "dragonmantank/cron-expression": "^2.3",
        "easycorp/easyadmin-bundle": "^2.3",
        "evence/soft-deleteable-extension-bundle": "^3.0",
        "gedmo/doctrine-extensions": "^2",
        "guzzlehttp/guzzle": "~6.0",
        "lexik/jwt-authentication-bundle": "^2.6",
        "liip/imagine-bundle": "^2.3",
        "mohammad-waleed/keycloak-admin-client": "^0.15.0",
        "oneup/flysystem-bundle": "^3.4",
        "ramsey/uuid": "^3.9",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/browser-kit": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/workflow": "5.1.*",
        "symfony/yaml": "5.1.*",
        "vich/uploader-bundle": "^1.13"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "escapestudios/symfony2-coding-standard": "3.10.0",
        "gree/jose": "^2.2",
        "hautelook/alice-bundle": "^2.7",
        "justinrainbow/json-schema": "^5.2",
        "koriym/json-schema-faker": "^0.1.2",
        "phpunit/php-code-coverage": "^7.0",
        "phpunit/phpunit": "^8",
        "rbdwllr/reallysimplejwt": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "swekaj/cron-expression-generator": "^1.0",
        "symfony/css-selector": "5.1.*",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    }
}
